<?xml version="1.0" encoding="UTF-8"?>
<dialog xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <flow>
        <folder label="Main">
            <output>
                <prompt selectionType="RANDOM">
                    <item>Hey!</item>
                </prompt>
                <goto ref="getUserInput_2449614"/>
            </output>
            <getUserInput id="getUserInput_2449614">
              <search ref="folder_2449611"/>
              <default>
                  <output>
                      <prompt selectionType="RANDOM">
                          <item>I am sorry, I did not understand.</item>
                      </prompt>
                  </output>
              </default>
            </getUserInput>
        </folder>
        <folder label="Library">
            <folder label="Live Content" id="folder_2449611">
              <input>
                <grammar>
                  <item>checkin</item>
                  <item>check in</item>
                  <item>haircut</item>
                  <item>hair cut</item>
                  <item>coming in</item>
                  <item>come in</item>
                  <item>be * there</item>
                  <item>$ checkin</item>
                  <item>$ check in</item>
                  <item>$ haircut</item>
                  <item>$ hair cut</item>
                  <item>$ coming in</item>
                  <item>$ come in</item>
                  <item>$ be * there</item>
                </grammar>
                <output>
                    <prompt selectionType="RANDOM">
                        <item>Sure, we cut a lot of hair!</item>
                    </prompt>
                    <goto ref="folder_details" />
                </output>
              </input>
            </folder>
            <folder label="Details" id="folder_details">
              <if>
                  <cond varName="User_Name" operator="IS_BLANK"/>
                  <output id="name_capture_input">
                    <prompt selectionType="RANDOM">
                        <item>What name should we use?</item>
                    </prompt>
                    <getUserInput id="name_capture">
                      <input>
                        <grammar>
                            <item>My name is</item>
                            <item>my name is (DYNAMIC_DATA)={User_Name}</item>
                            <item>mine is (DYNAMIC_DATA)={User_Name}</item>
                            <item>(DYNAMIC_DATA)={User_Name} </item>
                            <item>(DYNAMIC_DATA)={User_Name} GiveName</item>
                        </grammar>
                        <action varName="User_Name" operator="SET_TO">{User_Name.source}</action>
                        <output>
                          <prompt selectionType="RANDOM">
                            <item>Ok. Got it {User_Name}.</item>
                          </prompt>
                          <goto ref="folder_details" />
                        </output>
                      </input>
                    </getUserInput>
                  </output>
              </if>
              <if>
                  <cond varName="User_Location" operator="IS_BLANK"/>
                  <output id="location_capture_input">
                    <prompt selectionType="RANDOM">
                        <item>Which city?</item>
                    </prompt>
                    <getUserInput id="location_capture">
                      <input>
                        <grammar>
                            <item>I'm headed to</item>
                            <item>$ to (LOCATION)={User_Location}</item>
                            <item>$ going * (LOCATION)={User_Location}</item>
                            <item>$ in (LOCATION)={User_Location}</item>
                            <item>$ at (LOCATION)={User_Location}</item>
                            <item>(LOCATION)={User_Location}</item>
                            <item>$ (LOCATION)={User_Location}</item>
                        </grammar>
                        <action varName="User_Location" operator="SET_TO">{User_Location.source}</action>
                        <output>
                          <prompt selectionType="RANDOM">
                            <item>Perfect. {User_Location} is great.</item>
                          </prompt>
                          <if>
                            <cond varName="Prefer_Location" operator="EQUAL_TO_NO" />
                            <output>
                              <prompt>
                                <item>Would you like always use this location?</item>
                              </prompt>
                              <getUserInput id="capture_pref">
                                <input>
                                  <grammar>
                                    <item>yes</item>
                                    <item>$ yes</item>
                                  </grammar>
                                  <action varName="Prefer_Location" operator="SET_TO_YES" />
                                  <goto ref="folder_details" />
                                </input>
                                <input>
                                  <grammar>
                                    <item>no</item>
                                    <item>$ no</item>
                                  </grammar>
                                  <action varName="Prefer_Location" operator="SET_TO_NO" />
                                  <goto ref="folder_details" />
                                </input>
                              </getUserInput>
                            </output>
                          </if>
                          <goto ref="folder_details" />
                        </output>
                      </input>
                    </getUserInput>
                  </output>
              </if>
              <if matchType="ALL" >
                <cond varName="User_Location" operator="HAS_VALUE"/>
                <cond varName="User_Name" operator="HAS_VALUE"/>
                <output id="confirm_checkin">
                  <prompt selectionType="RANDOM">
                    <item>Ok {User_Name} you're all checked in at {User_Location}</item>
                  </prompt>
                  <if>
                    <cond varName="Prefer_Location" operator="EQUAL_TO_NO" />
                    <action varName="User_Location" operator="SET_TO_BLANK" />
                  </if>
                  <getUserInput id="await_terminate">
                    <input>
                      <grammar>
                        <item>bye</item>
                        <item>$ bye</item>
                        <item>$ thanks</item>
                        <item>thanks</item>
                        <item>cool</item>
                        <item>$ cool</item>
                        <item>$ awesome</item>
                      </grammar>
                      <output>
                        <prompt>
                          <item>later!</item>
                        </prompt>
                      </output>
                    </input>
                  </getUserInput>
                </output>
              </if>
            </folder>
            <folder label="Storage"/>
        </folder>
        <folder label="Global">
          <search ref="folder_2449611"/>
        </folder>
        <folder label="Concepts"/>
    </flow>
    <entities>
      <entity name="DYNAMIC_DATA" entityType="GENERIC">
          <value name="DataCapture" value="DataCapture">
              <grammar>
                  <item>*</item>
              </grammar>
          </value>
          <entityRules></entityRules>
      </entity>
    </entities>
    <constants>
        <var_folder name="Home"/>
    </constants>
    <variables>
        <var_folder name="Home"/>
        <var_folder name="CheckIn">
          <var name="User_Name" type="TEXT" />
          <var name="User_Location" type="TEXT" />
          <var name="Prefer_Location" type="YESNO" value="no"/>
        </var_folder>
    </variables>
